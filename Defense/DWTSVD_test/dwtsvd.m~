function img = dwtsvd(original, mark)

    [LL,LH,HL,HH] = dwt2(original,'haar');
    [WL,WH] = dwt(mark(:)','haar');
    
    [ULH,SLH,VLH] = svd(LH);
    [UHL,SHL,VHL] = svd(HL);
    %[~,WS,~] = svd(WHL);
    
    %startx = 1;
    %starty = 1;
    %[sizeix,sizeiy] = size(S);
    v1 = WL(1:256);
    v2 = WL(257:512);
    
    v3 = WH(1:256);
    v4 = WH(257:512);
    
    for
    
    SLH(1,:) = SLH(1,:) + v3;
    SLH(2,:) = SLH(2,:) + v4;
    
    SHL(1,:) = SHL(3,:) + v1;
    SHL(2,:) = SHL(4,:) + v2;
    
    LH = ULH*SLH*VLH';
    HL = UHL*SHL*VHL';
    
img = uint8(idwt2(LL,LH,HL,HH,'haar'));